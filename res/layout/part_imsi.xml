<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TableRow android:layout_width="match_parent"
              android:layout_height="wrap_content">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/category_imsi"
                android:layout_span="2"
                style="@style/Widget.Category"
                />
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="wrap_content">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/imsi"
                style="@style/Widget.Label"
                />
        <TextView
                android:id="@+id/text_imsi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/unknown"
                style="@style/Widget.Value"
                />
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="wrap_content">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/mcc"
                style="@style/Widget.Label"
                />
        <TextView
                android:id="@+id/text_mcc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/unknown"
                style="@style/Widget.Value"
                />
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="wrap_content">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/mnc"
                style="@style/Widget.Label"
                />
        <TextView
                android:id="@+id/text_mnc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/unknown"
                style="@style/Widget.Value"
                />
    </TableRow>
</merge>
